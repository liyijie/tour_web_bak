<h1>Listing admin_tour_orders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_tour_orders.each do |admin_tour_order| %>
      <tr>
        <td><%= link_to 'Show', admin_tour_order %></td>
        <td><%= link_to 'Edit', edit_admin_tour_order_path(admin_tour_order) %></td>
        <td><%= link_to 'Destroy', admin_tour_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

